<!--index.wxml-->

<view class='calendar-wrapper'>
 <calendar bindtabSelectDay="tabSelectDay"></calendar>
</view>

<view class='content-wrapper'>
  <view class='plan-item-wrapper' wx:for="{{planList}}" wx:key="index" >
    <planItem 
          plan="{{item}}" 
          bindtapPlanItem='tapPlanItem'>
    </planItem>
    
  </view>

</view>



<view class='bottom-tab-wrapper'>
  <bottomTab bindcatchItemTab='catchItemTab'></bottomTab>
</view>

